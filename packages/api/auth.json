{
  "providers": [
    {
      "name": "github"
    },
    {
      "name": "gitlab"
    },
    {
      "name": "codeberg",
      "enabled": false,
      "strategy": "gitea",
      "authorizationURL": "https://codeberg.org/login/oauth/authorize",
